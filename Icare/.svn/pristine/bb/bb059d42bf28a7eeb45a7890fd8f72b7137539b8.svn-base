package icare.dto;

import java.util.Date;

/**
 * 좌석
 * @author LMY
 *
 */
public class SeatVO {
	private Date time;	//시간
	private String sno;		//원생번호
	private int drno;		//기사번호
	private int seatno;		//좌석번호
	
	public SeatVO() {
	}

	public SeatVO(Date time, String sno, int drno, int seatno) {
		super();
		this.time = time;
		this.sno = sno;
		this.drno = drno;
		this.seatno = seatno;
	}

	public Date getTime() {
		return time;
	}

	public void setTime(Date time) {
		this.time = time;
	}

	public String getSno() {
		return sno;
	}

	public void setSno(String sno) {
		this.sno = sno;
	}

	public int getDrno() {
		return drno;
	}

	public void setDrno(int drno) {
		this.drno = drno;
	}

	public int getSeatno() {
		return seatno;
	}

	public void setSeatno(int seatno) {
		this.seatno = seatno;
	}

	@Override
	public String toString() {
		return "SeatVO [time=" + time + ", sno=" + sno + ", drno=" + drno + ", seatno=" + seatno + "]";
	}
}
