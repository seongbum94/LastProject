package icare.dto;
/**
 * 입소신청 정보가 담긴 VO
 * @author LMY
 *
 */
public class NonMemVO {
	private String nno;	//입소신청번호
	private String name;	//비회원학부모이름
	private String addr1;	//주소
	private String addr2;	//상세주소
	private int zip;		//우편번호
	private String email;	//이메일
	private String pwd;		//비밀번호
	private String phone;	//핸드폰번호
	private String kid;	//유치원번호
	
	public NonMemVO() {
	}

	public NonMemVO(String nno, String name, String addr1, String addr2, int zip, String email, String pwd,
			String phone, String kid) {
		super();
		this.nno = nno;
		this.name = name;
		this.addr1 = addr1;
		this.addr2 = addr2;
		this.zip = zip;
		this.email = email;
		this.pwd = pwd;
		this.phone = phone;
		this.kid = kid;
	}

	public String getNno() {
		return nno;
	}

	public void setNno(String nno) {
		this.nno = nno;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getAddr1() {
		return addr1;
	}

	public void setAddr1(String addr1) {
		this.addr1 = addr1;
	}

	public String getAddr2() {
		return addr2;
	}

	public void setAddr2(String addr2) {
		this.addr2 = addr2;
	}

	public int getZip() {
		return zip;
	}

	public void setZip(int zip) {
		this.zip = zip;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getPwd() {
		return pwd;
	}

	public void setPwd(String pwd) {
		this.pwd = pwd;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public String getKid() {
		return kid;
	}

	public void setKid(String kid) {
		this.kid = kid;
	}

	@Override
	public String toString() {
		return "NonMemVO [nno=" + nno + ", name=" + name + ", addr1=" + addr1 + ", addr2=" + addr2 + ", zip=" + zip
				+ ", email=" + email + ", pwd=" + pwd + ", phone=" + phone + ", kid=" + kid + "]";
	}
}
