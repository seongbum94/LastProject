package icare.dto;

import java.util.Date;

/**
 * 전자결재의 휴가신청서 서식
 * @author LMY
 *
 */
public class VacationVO {
	private String eno; // 문서번호
	private Date startdate; // 시작날짜
	private Date enddate; // 종료날짜
	private String dest; // 행선지

	public VacationVO() {
	}

	public VacationVO(String eno, Date startdate, Date enddate, String dest) {
		super();
		this.eno = eno;
		this.startdate = startdate;
		this.enddate = enddate;
		this.dest = dest;
	}

	public String getEno() {
		return eno;
	}

	public void setEno(String eno) {
		this.eno = eno;
	}

	public Date getStartdate() {
		return startdate;
	}

	public void setStartdate(Date startdate) {
		this.startdate = startdate;
	}

	public Date getEnddate() {
		return enddate;
	}

	public void setEnddate(Date enddate) {
		this.enddate = enddate;
	}

	public String getDest() {
		return dest;
	}

	public void setDest(String dest) {
		this.dest = dest;
	}

	@Override
	public String toString() {
		return "VacationVO [eno=" + eno + ", startdate=" + startdate + ", enddate=" + enddate + ", dest=" + dest + "]";
	}
}
