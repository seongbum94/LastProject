package icare.dto;

import java.util.Date;

/**
 * 컨텐츠공유
 * @author YSB
 */
public class ContentVO {
	private int conno;			//글번호
	private Date regdate;		//등록날짜
	private Date updatedate;	//수정날짜
	private int eno;			//문서번호

	public ContentVO() {
	}

	public ContentVO(int conno, Date regdate, Date updatedate, int eno) {
		super();
		this.conno = conno;
		this.regdate = regdate;
		this.updatedate = updatedate;
		this.eno = eno;
	}

	public int getConno() {
		return conno;
	}

	public void setConno(int conno) {
		this.conno = conno;
	}

	public Date getRegdate() {
		return regdate;
	}

	public void setRegdate(Date regdate) {
		this.regdate = regdate;
	}

	public Date getUpdatedate() {
		return updatedate;
	}

	public void setUpdatedate(Date updatedate) {
		this.updatedate = updatedate;
	}

	public int getEno() {
		return eno;
	}

	public void setEno(int eno) {
		this.eno = eno;
	}

	@Override
	public String toString() {
		return "ContentVO [conno=" + conno + ", regdate=" + regdate + ", updatedate=" + updatedate + ", eno=" + eno
				+ "]";
	}
}
