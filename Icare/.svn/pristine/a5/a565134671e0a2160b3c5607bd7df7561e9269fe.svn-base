package icare.dto;

import java.util.Date;

/**
 * 원생일지
 * @author LMY
 *
 */
public class StudentDiaryVO {
	private int dno;	//원생일지번호
	private Date regdate;	//작성일자
	private String physical;	//신체운동
	private String communication;	//의사소통
	private String social;	//사회관계
	private String art;		//예술경험
	private String nature;	//자연탐구
	private String sno;		//원생번호
	
	public StudentDiaryVO() {
		// TODO Auto-generated constructor stub
	}

	public StudentDiaryVO(int dno, Date regdate, String physical, String communication, String social, String art,
			String nature, String sno) {
		super();
		this.dno = dno;
		this.regdate = regdate;
		this.physical = physical;
		this.communication = communication;
		this.social = social;
		this.art = art;
		this.nature = nature;
		this.sno = sno;
	}

	public int getDno() {
		return dno;
	}

	public void setDno(int dno) {
		this.dno = dno;
	}

	public Date getRegdate() {
		return regdate;
	}

	public void setRegdate(Date regdate) {
		this.regdate = regdate;
	}

	public String getPhysical() {
		return physical;
	}

	public void setPhysical(String physical) {
		this.physical = physical;
	}

	public String getCommunication() {
		return communication;
	}

	public void setCommunication(String communication) {
		this.communication = communication;
	}

	public String getSocial() {
		return social;
	}

	public void setSocial(String social) {
		this.social = social;
	}

	public String getArt() {
		return art;
	}

	public void setArt(String art) {
		this.art = art;
	}

	public String getNature() {
		return nature;
	}

	public void setNature(String nature) {
		this.nature = nature;
	}

	public String getSno() {
		return sno;
	}

	public void setSno(String sno) {
		this.sno = sno;
	}

	@Override
	public String toString() {
		return "StudentDiaryVO [dno=" + dno + ", regdate=" + regdate + ", physical=" + physical + ", communication="
				+ communication + ", social=" + social + ", art=" + art + ", nature=" + nature + ", sno=" + sno + "]";
	}
}
