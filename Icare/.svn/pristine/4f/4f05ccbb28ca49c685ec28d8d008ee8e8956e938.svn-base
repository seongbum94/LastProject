package icare.dto;

import java.util.Date;

/**
 * 상담
 * @author YSB
 */
public class CounseilVO {
	private int counno;			//상담번호
	private Date coundate;      //날짜
	private Date time;        //상담시간
	private String type;        //상담종류
	private String tid;            //교번
	private String pid;         //학부모아이디
	
	public CounseilVO() {
	}

	public CounseilVO(int counno, Date coundate, Date time, String type, String tid, String pid) {
		super();
		this.counno = counno;
		this.coundate = coundate;
		this.time = time;
		this.type = type;
		this.tid = tid;
		this.pid = pid;
	}

	public int getCounno() {
		return counno;
	}

	public void setCounno(int counno) {
		this.counno = counno;
	}

	public Date getCoundate() {
		return coundate;
	}

	public void setCoundate(Date coundate) {
		this.coundate = coundate;
	}

	public Date getTime() {
		return time;
	}

	public void setTime(Date time) {
		this.time = time;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getTid() {
		return tid;
	}

	public void setTid(String tid) {
		this.tid = tid;
	}

	public String getPid() {
		return pid;
	}

	public void setPid(String pid) {
		this.pid = pid;
	}

	@Override
	public String toString() {
		return "CounseilVO [counno=" + counno + ", coundate=" + coundate + ", time=" + time + ", type=" + type
				+ ", tid=" + tid + ", pid=" + pid + "]";
	}
	
}
