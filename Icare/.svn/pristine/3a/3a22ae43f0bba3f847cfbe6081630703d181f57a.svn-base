package icare.dto;


/**
 * 코스
 * @author YSB
 */
public class CourseVO {
	private int courseno;		//코스번호
	private int drno;			//기사번호
	
	public CourseVO() {
		// TODO Auto-generated constructor stub
	}

	public CourseVO(int courseno, int drno) {
		super();
		this.courseno = courseno;
		this.drno = drno;
	}

	public int getCourseno() {
		return courseno;
	}

	public void setCourseno(int courseno) {
		this.courseno = courseno;
	}

	public int getDrno() {
		return drno;
	}

	public void setDrno(int drno) {
		this.drno = drno;
	}

	@Override
	public String toString() {
		return "CourseVO [courseno=" + courseno + ", drno=" + drno + "]";
	}
	
}
