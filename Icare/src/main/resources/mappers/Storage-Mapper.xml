<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Storage-Mapper">
	
	<update id="deleteStorage" parameterType="storage">
		delete
		  from storage
		 where tid = #{tid}
		   and conno = #{conno}
	</update>
	
	
</mapper>